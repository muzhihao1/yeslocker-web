{
  "name": "yeslocker",
  "version": "1.0.0",
  "description": "台球杆柜管理小程序 - 前端用户端",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:h5": "vite build",
    "start": "node minimal-test.js",
    "dev:admin": "cd admin && npm run dev",
    "build:admin": "cd admin && npm run build",
    "server": "cd server && npm start",
    "server:dev": "cd server && npm run dev",
    "server:unified": "cd server && npm run start:unified",
    "server:unified:dev": "cd server && npm run dev:unified",
    "server:pg": "cd server && npm run start:pg",
    "server:pg:dev": "cd server && npm run dev:pg",
    "dev:all": "npm run server & npm run dev",
    "db:migrate": "supabase db push",
    "db:reset": "supabase db reset",
    "db:migrate:pg": "cd server && node database/migrate-data.js",
    "functions:serve": "supabase functions serve",
    "functions:deploy": "supabase functions deploy",
    "functions:deploy:dev": "./supabase/deploy-functions.sh development",
    "functions:deploy:staging": "./supabase/deploy-functions.sh staging",
    "functions:deploy:prod": "./supabase/deploy-functions.sh production",
    "functions:logs": "supabase functions logs",
    "functions:test": "cd supabase && deno test --allow-all functions/**/*test.ts",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@dcloudio/uni-app": "^3.0.0-alpha-4070620250731001",
    "@dcloudio/uni-cli-shared": "^3.0.0-alpha-4070620250731001",
    "@dcloudio/uni-components": "^3.0.0-alpha-4070620250731001",
    "@dcloudio/uni-h5": "^3.0.0-alpha-4070620250731001",
    "@dcloudio/vite-plugin-uni": "^3.0.0-alpha-4070620250731001",
    "@supabase/functions-js": "^2.1.1",
    "@supabase/supabase-js": "^2.38.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "axios": "^1.3.4",
    "dayjs": "^1.11.7",
    "express": "^4.21.2",
    "pinia": "^2.0.30",
    "qrcode": "^1.5.3",
    "vite": "^5.2.8",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@dcloudio/types": "^3.3.2",
    "@dcloudio/uni-automator": "^3.0.0-alpha-4070620250731001",
    "@dcloudio/uni-stacktracey": "^3.0.0-alpha-4070620250731001",
    "@types/node": "^18.14.0",
    "@vue/runtime-core": "^3.2.45",
    "@vue/tsconfig": "^0.1.3",
    "typescript": "^4.9.5",
    "vue-tsc": "^1.0.24"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "uni-app",
    "vue3",
    "typescript",
    "台球",
    "管理系统"
  ],
  "author": "Terminal 1 Developer",
  "license": "MIT"
