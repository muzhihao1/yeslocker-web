{
  "name": "yeslocker",
  "version": "1.0.0",
  "description": "台球杆柜管理小程序 - 前端用户端",
  "main": "index.js",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 3000",
    "build": "npm run build:client && npm run build:admin && npm run build:server",
    "build:client": "vite build",
    "build:admin": "cd admin && npm install && npm run build",
    "build:server": "cd server && npm install",
    "preview": "vite preview",
    "start": "node server/index-railway.js",
    "dev:admin": "cd admin && npm run dev",
    "dev:server": "cd server && npm run dev",
    "dev:all": "npm run dev:server & npm run dev",
    "db:init": "cd server && npm run db:init",
    "db:migrate": "cd server && node database/migrate-data.js",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "axios": "^1.3.4",
    "bcrypt": "^5.1.1",
    "colors": "^1.4.0",
    "connect-history-api-fallback": "^2.0.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "path": "^0.12.7",
    "pg": "^8.11.3",
    "pinia": "^2.0.30",
    "qrcode": "^1.5.3",
    "vue": "^3.2.45",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@types/node": "^18.14.0",
    "@vue/runtime-core": "^3.2.45",
    "@vue/tsconfig": "^0.1.3",
    "nodemon": "^3.0.0",
    "typescript": "^4.9.5",
    "vite": "^5.2.8",
    "vue-tsc": "^1.0.24"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "vue3",
    "vite",
    "typescript",
    "台球",
    "管理系统",
    "express",
    "sqlite"
  ],
  "author": "Terminal 1 Developer",
  "license": "MIT"
}
