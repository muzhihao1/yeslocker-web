{
  "dashboard": {
    "id": null,
    "title": "YesLocker - System Overview",
    "tags": ["yeslocker", "overview", "production"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Service Status Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"yeslocker-api\"}",
            "legendFormat": "API Status",
            "refId": "A"
          },
          {
            "expr": "up{job=\"yeslocker-admin\"}",
            "legendFormat": "Admin Status",
            "refId": "B"
          },
          {
            "expr": "up{job=\"postgres-exporter\"}",
            "legendFormat": "Database Status", 
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "DOWN",
                    "color": "red"
                  },
                  "1": {
                    "text": "UP",
                    "color": "green"
                  }
                },
                "type": "value"
              }
            ],
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "green", 
                  "value": 1
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 24,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Request Rate (req/s)",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{job=\"yeslocker-api\"}[5m])",
            "legendFormat": "{{method}} {{endpoint}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Requests/sec"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 4
        }
      },
      {
        "id": 3,
        "title": "Response Time (95th percentile)",
        "type": "graph", 
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=\"yeslocker-api\"}[5m]))",
            "legendFormat": "95th percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket{job=\"yeslocker-api\"}[5m]))",
            "legendFormat": "Median",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Response Time (s)"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 4
        }
      },
      {
        "id": 4,
        "title": "Error Rate (%)",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{job=\"yeslocker-api\",status=~\"5..\"}[5m]) / rate(http_requests_total{job=\"yeslocker-api\"}[5m]) * 100",
            "legendFormat": "5xx Error Rate",
            "refId": "A"
          },
          {
            "expr": "rate(http_requests_total{job=\"yeslocker-api\",status=~\"4..\"}[5m]) / rate(http_requests_total{job=\"yeslocker-api\"}[5m]) * 100",
            "legendFormat": "4xx Error Rate",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Error Rate (%)",
            "max": 100
          }
        ],
        "thresholds": [
          {
            "value": 5,
            "colorMode": "critical",
            "op": "gt"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 12
        }
      },
      {
        "id": 5,
        "title": "Active Users (Last 24h)",
        "type": "stat",
        "targets": [
          {
            "expr": "active_users_24h",
            "legendFormat": "Active Users",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 50
                },
                {
                  "color": "green",
                  "value": 100
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 12,
          "y": 12
        }
      },
      {
        "id": 6,
        "title": "Locker Utilization",
        "type": "gauge",
        "targets": [
          {
            "expr": "occupied_lockers_total / total_lockers_available * 100",
            "legendFormat": "Utilization %",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 50
                },
                {
                  "color": "green",
                  "value": 80
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 18,
          "y": 12
        }
      },
      {
        "id": 7,
        "title": "Database Metrics",
        "type": "graph",
        "targets": [
          {
            "expr": "pg_stat_activity_count",
            "legendFormat": "Active Connections",
            "refId": "A"
          },
          {
            "expr": "rate(pg_stat_database_tup_inserted[5m])",
            "legendFormat": "Inserts/sec",
            "refId": "B"
          },
          {
            "expr": "rate(pg_stat_database_tup_updated[5m])",
            "legendFormat": "Updates/sec",
            "refId": "C"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 20
        }
      },
      {
        "id": 8,
        "title": "System Resources",
        "type": "graph",
        "targets": [
          {
            "expr": "100 - (avg(rate(node_cpu_seconds_total{mode=\"idle\"}[5m])) * 100)",
            "legendFormat": "CPU Usage %",
            "refId": "A"
          },
          {
            "expr": "(node_memory_MemTotal_bytes - node_memory_MemAvailable_bytes) / node_memory_MemTotal_bytes * 100",
            "legendFormat": "Memory Usage %",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Usage %",
            "max": 100
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 20
        }
      },
      {
        "id": 9,
        "title": "Business Metrics - Daily",
        "type": "graph",
        "targets": [
          {
            "expr": "increase(user_registrations_total[1d])",
            "legendFormat": "New Registrations",
            "refId": "A"
          },
          {
            "expr": "increase(applications_total[1d])",
            "legendFormat": "New Applications",
            "refId": "B"
          },
          {
            "expr": "increase(application_approvals_total[1d])",
            "legendFormat": "Approvals",
            "refId": "C"
          },
          {
            "expr": "increase(locker_operations_total[1d])",
            "legendFormat": "Locker Operations",
            "refId": "D"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 28
        }
      },
      {
        "id": 10,
        "title": "Security Events",
        "type": "table",
        "targets": [
          {
            "expr": "increase(failed_login_attempts_total[1h])",
            "legendFormat": "Failed Logins (1h)",
            "refId": "A",
            "format": "table"
          },
          {
            "expr": "increase(rate_limit_triggered_total[1h])",
            "legendFormat": "Rate Limits (1h)",
            "refId": "B",
            "format": "table"
          },
          {
            "expr": "increase(admin_login_failures_total[1h])",
            "legendFormat": "Admin Login Failures (1h)",
            "refId": "C",
            "format": "table"
          }
        ],
        "transformations": [
          {
            "id": "merge",
            "options": {}
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 36
        }
      },
      {
        "id": 11,
        "title": "Top Endpoints by Response Time",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=\"yeslocker-api\"}[5m])))",
            "legendFormat": "{{method}} {{endpoint}}",
            "refId": "A",
            "format": "table"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 36
        }
      }
    ],
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "prometheus",
          "enable": true,
          "expr": "changes(up{job=\"yeslocker-api\"}[5m]) > 0",
          "iconColor": "blue",
          "textFormat": "Deployment"
        }
      ]
    },
    "templating": {
      "list": [
        {
          "name": "instance",
          "type": "query",
          "query": "label_values(up{job=\"yeslocker-api\"}, instance)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*"
        },
        {
          "name": "endpoint",
          "type": "query", 
          "query": "label_values(http_requests_total{job=\"yeslocker-api\"}, endpoint)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*"
        }
      ]
    }
  }
}